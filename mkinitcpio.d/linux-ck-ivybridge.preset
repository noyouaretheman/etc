# mkinitcpio preset file for the 'linux-ck-ivybridge' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-ck-ivybridge"

PRESETS=('default')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-ck-ivybridge.img"
#default_options=""

fallback_config="/etc/mkinitcpio-fallback.conf"
fallback_image="/boot/initramfs-linux-ck-ivybridge-fallback.img"
fallback_options="-S autodetect"
